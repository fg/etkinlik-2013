            {% block top %}{% endblock %}

            {% block body %}{% endblock %}

            <div id="fakeFooter"></div>
        </div> <!-- #contentAboveFooter -->

        <footer>
            <span id="mascot" class="sprite ir">Maskot</span>
            <div class="toCenter">
                <small>{% trans %}footer.info{% endtrans %}</small>

                <div id="designer">
                    {% trans with {'%link%':'<a href="#">Ethem Budak</a>'} %}footer.designer{% endtrans %}
                </div>
            </div> <!-- .toCenter -->
